{
Realizar un programa que imprima 25 t√©rminos de la serie 11 - 22 - 33 - 44, etc.
(No se ingresan valores por teclado)
}

program maxi;

  var
       x,contador : integer;
begin
   x:= 11;
   contador:= 1;
   while contador <= 25 do
    begin
      writeln(x,' - ');
      x:= x + 11;
      contador:= contador + 1;
    end;
   readln;

end.
